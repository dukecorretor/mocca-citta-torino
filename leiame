<script>
document.addEventListener("DOMContentLoaded", function() {
  const btn = document.getElementById("toggle-comentarios");
  const lista = document.getElementById("comentarios-lista");
  let visivel = true;
  lista.style.display = "block";
  btn.addEventListener("click", function() {
    visivel = !visivel;
    lista.style.display = visivel ? "block" : "none";
  });
  // Formul√°rio de coment√°rio: agradecimento inline
  const form = document.getElementById("comentarioForm");
  const agradecimento = document.getElementById("comentarioAgradecimento");
  if(form) {
    form.addEventListener("submit", function(e) {
      e.preventDefault();
      // Envia para Netlify
      const data = new FormData(form);
      fetch("/", {
        method: "POST",
        body: data
      }).then(function() {
        agradecimento.style.display = "block";
        form.reset();
        setTimeout(()=>{ agradecimento.style.display = "none"; }, 8000);
      });
    });
  }
});
</script>

