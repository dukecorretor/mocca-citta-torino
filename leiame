<!-- JavaScript Atualizado -->
<script>
  const botoes = document.querySelectorAll('.categoria-btn');
  const galerias = document.querySelectorAll('.carrossel-galeria');
  const imagens = {
    apartamento: ['dorm-meio.jpg', 'dorm-ponta.jpg'],
    plantas: ['planta-meio.jpg', 'planta-ponta.jpg', 'planta-ponta-opcao.jpg', 'implantacao.jpg'],
    lazer: ['moved-osasco-lazer1.jpg', 'moved-osasco-lazer2.jpg', 'moved-osasco-lazer3.jpg', 'moved-osasco-lazer4.jpg', 'moved-osasco-lazer5.jpg', 'moved-osasco-lazer6.jpg', 'moved-osasco-lazer7.jpg', 'moved-osasco-lazer8.jpg', 'moved-osasco-lazer9.jpg', 'moved-osasco-lazer10.jpg', 'moved-osasco-lazer11.jpg', 'moved-osasco-lazer12.jpg', 'moved-osasco-lazer13.jpg', 'moved-osasco-lazer14.jpg', 'moved-osasco-lazer15.jpg', 'moved-osasco-lazer16.jpg', 'moved-osasco-lazer17.jpg', 'moved-osasco-lazer18.jpg', 'moved-osasco-lazer19.jpg', 'moved-osasco-lazer20.jpg']
  };
  const indices = { apartamento: 0, plantas: 0, lazer: 0 };
  // Inicializa com apartamento ativo
  document.addEventListener('DOMContentLoaded', function() {
    botoes.forEach(btn => btn.classList.remove('active'));
    galerias.forEach(galeria => galeria.classList.remove('active'));
    document.querySelector('.categoria-btn[data-categoria="apartamento"]').classList.add('active');
    document.getElementById('apartamento').classList.add('active');
    atualizarImagem('apartamento');
  });
  botoes.forEach(botao => {
    botao.addEventListener('click', () => {
      botoes.forEach(btn => btn.classList.remove('active'));
      galerias.forEach(galeria => galeria.classList.remove('active'));
      botao.classList.add('active');
      const categoria = botao.getAttribute('data-categoria');
      document.getElementById(categoria).classList.add('active');
      atualizarImagem(categoria);
    });
  });
  window.navegar = function(categoria, direcao) {
    const total = imagens[categoria].length;
    indices[categoria] = (indices[categoria] + direcao + total) % total;
    atualizarImagem(categoria);
  }
  function atualizarImagem(categoria) {
    document.getElementById(`img-${categoria}`).src = imagens[categoria][indices[categoria]];
  }

  // --- Modal/Lightbox ---
  let modalAberto = false;
  let categoriaAtual = 'apartamento';
  let indiceAtual = 0;
  const modal = document.getElementById('modal-lightbox');
  const modalImg = document.getElementById('modal-img');
  const modalPrev = document.getElementById('modal-prev');
  const modalNext = document.getElementById('modal-next');
  const modalClose = document.getElementById('modal-close');

  // Detecta qual galeria est√° ativa
  function getCategoriaAtiva() {
    for (const galeria of galerias) {
      if (galeria.classList.contains('active')) return galeria.id;
    }
    return 'apartamento';
  }

  // Abre o modal ao clicar na imagem do carrossel
  document.querySelectorAll('.carrossel-img').forEach(img => {
    img.addEventListener('click', function() {
      categoriaAtual = getCategoriaAtiva();
      indiceAtual = indices[categoriaAtual];
      abrirModal();
    });
  });

  function abrirModal() {
    modal.classList.add('active');
    modalAberto = true;
    atualizarModalImg();
    document.body.style.overflow = 'hidden'; // Evita scroll do fundo
  }
  function fecharModal() {
    modal.classList.remove('active');
    modalAberto = false;
    document.body.style.overflow = '';
  }
  function atualizarModalImg() {
    modalImg.src = imagens[categoriaAtual][indiceAtual];
    modalImg.alt = 'Imagem em tela cheia';
  }
  modalPrev.addEventListener('click', function(e) {
    e.stopPropagation();
    const total = imagens[categoriaAtual].length;
    indiceAtual = (indiceAtual - 1 + total) % total;
    atualizarModalImg();
  });
  modalNext.addEventListener('click', function(e) {
    e.stopPropagation();
    const total = imagens[categoriaAtual].length;
    indiceAtual = (indiceAtual + 1) % total;
    atualizarModalImg();
  });
  modalClose.addEventListener('click', function(e) {
    e.stopPropagation();
    fecharModal();
  });
  // Fecha ao clicar fora da imagem
  modal.addEventListener('click', function(e) {
    if (e.target === modal) fecharModal();
  });
  // Fecha com ESC
  document.addEventListener('keydown', function(e) {
    if (modalAberto && e.key === 'Escape') fecharModal();
  });
</script>
</section>
